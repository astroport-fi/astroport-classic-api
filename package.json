{
  "name": "astroport-api",
  "version": "1.0.0",
  "main": "src/index.js",
  "author": "Arthur Itey",
  "private": true,
  "engines": {
    "node": ">=12.3.1",
    "npm": ">=6.9.0"
  },
  "type": "commonjs",
  "scripts": {
    "dev": "serverless offline --useChildProcesses",
    "database:clean": "rimraf mongodb_data",
    "database": "mkdir -p mongodb_data/rs0-0 ; mongod --dbpath=mongodb_data/rs0-0 --replSet rs0 --port 27050 --oplogSize 128 --profile=1 --slowms=1",
    "database:init": "mongo --port 27050 tools/init-mongodb-rs.js",
    "eslint": "eslint --quiet --max-warnings 0 --ext .tsx --ext .ts --ext .js --ext .jsx .",
    "prettier": "prettier --list-different '**/*.{ts,tsx,js,jsx,json,css,md}'",
    "test": "mocha --timeout 10000 -r ts-node/register test/**/*.ts",
    "typescript": "tsc --noEmit"
  },
  "dependencies": {
    "@graphql-tools/schema": "^8.3.0",
    "@terra-money/log-finder": "^1.1.6",
    "@terra-money/terra.js": "^3.0.2",
    "@types/aws-lambda": "^8.10.89",
    "@types/chai": "^4.3.0",
    "@types/mocha": "^9.0.0",
    "apollo-server": "^3.5.0",
    "apollo-server-lambda": "^3.5.0",
    "bignumber.js": "^9.0.2",
    "bluebird": "^3.7.2",
    "chai": "^4.3.4",
    "dayjs": "^1.10.7",
    "graphql": "^16.2.0",
    "graphql-request": "^3.7.0",
    "graphql-scalars": "^1.12.0",
    "husky": "7.0.1",
    "lodash": "^4.17.21",
    "mongodb": "^3.6.9",
    "mongoose": "^6.1.3",
    "node-fetch": "^3.1.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.5.4"
  },
  "devDependencies": {
    "@graphql-eslint/eslint-plugin": "^3.3.0",
    "@types/bluebird": "^3.5.36",
    "@types/jest": "^26.0.23",
    "@types/lodash": "^4.14.170",
    "@types/mongodb": "^3.6.19",
    "@typescript-eslint/eslint-plugin": "4.28.2",
    "eslint": "7.30.0",
    "eslint-config-typescript": "^3.0.0",
    "jest": "^27.0.6",
    "lint-staged": "^11.0.0",
    "mocha": "^9.1.3",
    "prettier": "^2.3.2",
    "serverless-dotenv-plugin": "^3.10.0",
    "serverless-offline": "^8.2.0",
    "serverless-plugin-typescript": "^2.1.0",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.4.0"
  },
  "prettier": {
    "singleQuote": false
  }
}
